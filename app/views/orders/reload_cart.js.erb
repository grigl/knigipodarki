<% unless @order.line_items.empty? %>
  $('.cart .panel_in .items').html('<%= escape_javascript(render "shared/cart_quantity") %>');
  $('.cart .forsale').html('<%= escape_javascript(render "shared/cart_items") %>');
  $('.cart').show();
<% else %>
  $(".sp").removeClass("active");
  $(".sp_show").hide();
  $(".cart").hide();
<% end %>